# Agent System Configuration Example
# Copy this file to .env and fill in your API keys

# API Keys
ANTHROPIC_API_KEY=your-anthropic-api-key-here
OPENAI_API_KEY=your-openai-api-key-here  # Optional

# Model Configuration
ANTHROPIC_MODEL=claude-3-opus-20240229  # or claude-3-sonnet-20240229
OPENAI_MODEL=gpt-4  # or gpt-4-turbo-preview

# System Configuration
LOG_LEVEL=INFO  # DEBUG, INFO, WARNING, ERROR
RAG_COLLECTION_NAME=code_chunks
QDRANT_HOST=localhost
QDRANT_PORT=6333

# Optional: Message Queue Configuration
# Uncomment if using Kafka or RabbitMQ
# KAFKA_BOOTSTRAP_SERVERS=localhost:9092
# AMQP_URL=amqp://guest:guest@localhost:5672/