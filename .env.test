# Test Environment Configuration
# Used for E2E integration tests

# Test-specific settings
AGENT_LOG_LEVEL=DEBUG
AGENT_DEBUG=true

# Use in-memory backends for testing
AGENT_MESSAGING_BACKEND=memory
AGENT_CACHE_BACKEND=memory

# RAG settings for testing
AGENT_RAG_VECTOR_STORE_TYPE=chroma
USE_ADAPTIVE_RAG=true
ADAPTIVE_RATE=0.5  # Faster adaptation for tests

# Smaller limits for testing
AGENT_LLM_MAX_TOKENS=2000
AGENT_LLM_REQUEST_TIMEOUT_SECONDS=30

# Test models (will use mocks if no API keys)
ARCHITECT_MODEL=gpt-4o
REQUEST_PLANNER_MODEL=gpt-4o
CODING_MODEL=gpt-4o
ANALYZER_MODEL=gpt-4o

# Disable external integrations
AGENT_WEBHOOK_ENABLED=false

# Cost limits for testing
AGENT_MAX_COST_PER_SESSION=1.00